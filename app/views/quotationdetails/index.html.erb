<p id="notice"><%= notice %></p>

<h1>Quotationdetails</h1>

<table>
  <thead>
    <tr>
      <th>Quotation</th>
      <th>Brand</th>
      <th>Product</th>
      <th>Color</th>
      <th>Quantity</th>
      <th>Size</th>
      <th>Ratio</th>
      <th>Deliverydate</th>
      <th>Targetprice</th>
      <th>Finalprice</th>
      <th>Otherdetails</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @quotationdetails.each do |quotationdetail| %>
      <tr>
        <td><%= quotationdetail.quotation.id %></td>
        <td><%= quotationdetail.brand.name %></td>
        <td><%= quotationdetail.product.description %></td>
        <td><%= quotationdetail.color.name %></td>
        <td><%= quotationdetail.quantity %></td>
        <td><%= quotationdetail.size.name %></td>
        <td><%= quotationdetail.ratio %></td>
        <td><%= quotationdetail.deliverydate %></td>
        <td><%= quotationdetail.targetprice %></td>
        <td><%= quotationdetail.finalprice %></td>
        <td><%= quotationdetail.otherdetails %></td>
        <td><%= link_to 'Show', quotationdetail %></td>
        <td><%= link_to 'Edit', edit_quotationdetail_path(quotationdetail) %></td>
        <td><%= link_to 'Destroy', quotationdetail, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Quotationdetail', new_quotationdetail_path %>
