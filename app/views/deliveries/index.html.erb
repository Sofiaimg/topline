<p id="notice"><%= notice %></p>

<h1>Deliveries</h1>

<table>
  <thead>
    <tr>
      <th>Order</th>
      <th>Port</th>
      <th>Invnumber</th>
      <th>Invdate</th>
      <th>Totalinvq</th>
      <th>Price</th>
      <th>Totalinvamount</th>
      <th>Eta</th>
      <th>Bl</th>
      <th>Docs</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @deliveries.each do |delivery| %>
      <tr>
        <td><%= delivery.order %></td>
        <td><%= delivery.port %></td>
        <td><%= delivery.invnumber %></td>
        <td><%= delivery.invdate %></td>
        <td><%= delivery.totalinvq %></td>
        <td><%= delivery.price %></td>
        <td><%= delivery.totalinvamount %></td>
        <td><%= delivery.eta %></td>
        <td><%= delivery.bl %></td>
        <td><%= delivery.docs %></td>
        <td><%= link_to 'Show', delivery %></td>
        <td><%= link_to 'Edit', edit_delivery_path(delivery) %></td>
        <td><%= link_to 'Destroy', delivery, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Delivery', new_delivery_path %>
