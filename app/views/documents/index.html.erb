<p id="notice"><%= notice %></p>

<h1>Documents</h1>

<table>
  <thead>
    <tr>
      <th>Delivery</th>
      <th>Docsbymail</th>
      <th>Approvaldate</th>
      <th>Coriername</th>
      <th>Couriernumber</th>
      <th>Docsreceived</th>
      <th>Docssenttoclient</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @documents.each do |document| %>
      <tr>
        <td><%= document.delivery %></td>
        <td><%= document.docsbymail %></td>
        <td><%= document.approvaldate %></td>
        <td><%= document.coriername %></td>
        <td><%= document.couriernumber %></td>
        <td><%= document.docsreceived %></td>
        <td><%= document.docssenttoclient %></td>
        <td><%= link_to 'Show', document %></td>
        <td><%= link_to 'Edit', edit_document_path(document) %></td>
        <td><%= link_to 'Destroy', document, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Document', new_document_path %>
